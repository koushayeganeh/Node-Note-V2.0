<div class="main-container">
  <div class="container">
    <h1 class="item"><%= userName + `'s ` %> Notes</h1>
    <div class="item notes-container">Here is the Notes container</div>
    <div class="notes-container">
      <% if (notes.length > 0) { %> <% notes.forEach(note => { %>
      <div class="note-preview">
        <h2><%= note.title %></h2>
        <p><%= note.body %></p>
      </div>
      <% }) %> <% } else { %>
      <p>No notes found.</p>
      <% } %>
    </div>
  </div>
</div>
