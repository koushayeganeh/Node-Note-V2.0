<div class="main-container">
  <div class="container">
    <h1 class="item"><%= userName + `'s ` %> Notes</h1>
    <div class="item notes-container">Here is the Notes container</div>
    <div class="notes-container">
      <% if (notes.length > 0) { %> <% notes.forEach(note => { %>
      <a href="/dashboard/note/<%= note._id %>" class="note-preview">
        <h3><%= note.title %></h3>
        <p><%= note.body %></p>
      </a>
      <% }) %> <% } else { %>
      <p>No notes found.</p>
      <% } %>
    </div>
  </div>
</div>
